{
  "manifest_version": 2,

  "name": "Hide Care React on Facebook",
  "description": "Hide Care React on Facebook",
  "version": "1.0",
  "content_scripts": [
    {
      "matches": ["https://*.facebook.com/*", "https://www.facebook.com/*", "http://www.facebook.com/*", "http://*.facebook.com/*"],
      "css": ["hide-care-react.css"]
    }
  ],
  "permissions": [
   "activeTab"
   ]
}